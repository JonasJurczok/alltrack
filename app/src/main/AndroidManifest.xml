<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.linesofcode.alltrack">

	<application android:label="@string/app_name"
		android:allowBackup="true"
		android:theme="@style/AppTheme"
		android:name=".App"
		android:icon="@drawable/app_icon">

		<activity android:name=".graph.GraphActivity"
		android:label="@string/app_name">
		<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".graph.CreateGraphActivity"
			android:label="@string/graph_detail_title_create"/>
		<activity android:name=".graph.AddValueActivity"
			android:label="@string/add_value_title"/>
        <activity android:name=".SettingsActivity"
            android:label="@string/settings_activity_label"/>
	</application>
</manifest>
